import lombok.NoArgsConstructor;

@Data
@AllArgsConstructor
@NoArgsConstructor
public class Qna {
	private int no;
	private String title;
	private String content;
	private int hits;
	private Date resdate;
	private String id;
	private String name;
	private String cate;	//member의 등급이 cate일때 답변을 달거나 수정 및 삭제를 할 수 있도록 기능구현하기 위해 cate사용
	private int plevel;
	private int parno;
	private String img1;	//질문이미지1
	private String img2;	//질문이미지2
	private String img3;	//답변이미지1
	private String img4;	//답변이미지2
		
}
