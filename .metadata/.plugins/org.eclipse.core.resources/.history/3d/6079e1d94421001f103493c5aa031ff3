package com.project.biz;

import java.util.List;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.company.dto.Board;
import com.project.per.NoticeDAO;
@Service
public class NoticeServiceImpl implements NoticeService{

	@Autowired
	private NoticeDAO noticeDAO;
	
	@Override
	public List<Notice> getBoardList() {
		return noticeDAO.getBoardList();
	}

	@Override
	public Board getBoard(int bno) {
		return noticeDAO.getBoard(bno);
	}

	@Override
	public void insBoard(Board board) {
		noticeDAO.insBoard(board);
		
	}

	@Override
	public void upBoard(Board board) {
		noticeDAO.upBoard(board);
		
	}

	@Override
	public void delNotice(int nno) {
		noticeDAO.delNotice(nno);
		
	}
	
}
