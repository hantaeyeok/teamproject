package com.project.domain;

import lombok.AllArgsConstructor;
import lombok.Data;
import lombok.NoArgsConstructor;

@Data
@AllArgsConstructor
@NoArgsConstructor
public class Faq {
	private int no;
	private String title;
	private String content;	
	private String mcate;	//member cate랑 동일 사용방법 : cate가 admin일때 등록 및 수정이 가능하도록.
}
